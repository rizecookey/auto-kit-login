{
    "manifest_version": 3,
    "name": "Auto KIT Login",
    "description": "Automatic Shibboleth login for KIT pages",
    "version": "0.2.1",
    "host_permissions": [
        "*://*.kit.edu/"
    ],
    "action": {
        "default_icon": "kit_cookie.png",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "webNavigation",
        "webRequest",
        "cookies",
        "storage",
        "scripting"
    ],
    "background": {
        "service_worker": "background/background.js"
    },
    "icons": {
        "822": "kit_cookie.png"
    }
}